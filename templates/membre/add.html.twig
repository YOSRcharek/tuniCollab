

<!DOCTYPE html>
<html :class="{ 'theme-dark': dark }" x-data="data()" lang="en">
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>TuniCollab Dashboard</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    <link rel="icon" type="image/x-icon" href="{{ asset('assets/front/images/grandLogo.png') }}" />
    
    {% block stylesheets %}
    <link href="{{ asset('assets/front/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/css/bootstrap-icons.css') }}" rel="stylesheet">
    <link href="{{ asset('assets/front/css/templatemo-kind-heart-charity.css') }}" rel="stylesheet">
    {% endblock %}

    {% block javascripts %}
    <script src="{{ asset('assets/front/js/jquery.min.js') }}"></script>
    <script src="{{ asset('assets/front/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('assets/front/js/jquery.sticky.js') }}"></script>
    <script src="{{ asset('assets/front/js/custom.js') }}"></script>
    <script src="{{ asset('assets/front/js/counter.js') }}"></script>
    <script src="{{ asset('assets/front/js/click-scroll.js') }}"></script>
    {% endblock %}
     <link
      href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700;800&display=swap"
      rel="stylesheet"
    />
    <link rel="stylesheet" href="{{ asset('assets/back/css/tailwind.output.css') }}" />
    <script
      src="https://cdn.jsdelivr.net/gh/alpinejs/alpine@v2.x.x/dist/alpine.min.js"
      defer
    ></script>
    <script src="assets/back/js/init-alpine.js"></script>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.css"
    />
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.js"
      defer
    ></script>
    <script src="{{ asset('assets/back/js/charts-lines.js') }}" defer></script>
    <script src="{{ asset('assets/back/js/charts-pie.js') }}" defer></script>
  </head>
  <body>
   
   <div class="flex h-screen bg-gray-50 dark:bg-gray-900":class="{ 'overflow-hidden': isSideMenuOpen }">
     {{ include('admin/sidebar.html.twig') }}

    <div
      x-show="isSideMenuOpen"
    >
   </div>
    <div class="flex flex-col flex-1 w-full">

      {{ include('admin/header.html.twig') }}

{% block body %}
<div class="container p-5 my-5 border">
              <h1>Add projet</h1>
              <div class="row">
                {{ form_start(form, {'attr': {'class': 'form '}}) }}
             
                 <div class="col-lg-6 col-12">
                     {{ form_row(form.nomMembre, {'label': 'Nom:', 'attr': {'class': 'form-control', 'placeholder': 'Nom', 'required': 'required'}}) }}
                     {# Afficher les erreurs spécifiques au champ nomMembre #}
                    
                 </div>
                 
                 <div class="col-lg-6 col-12">
                     {{ form_row(form.prenomMembre, {'label': 'Prénom:', 'attr': {'class': 'form-control', 'placeholder': 'Prénom', 'required': 'required'}}) }}
                     {# Afficher les erreurs spécifiques au champ prenomMembre #}
                   
                 </div>
                 
                 <div class="col-lg-6 col-12">
                     {{ form_row(form.telephone, {'label': 'Téléphone:', 'attr': {'class': 'form-control', 'placeholder': 'Téléphone', 'required': 'required'}}) }}
                     {# Afficher les erreurs spécifiques au champ telephone #}
                    
                 
                 <div class="col-lg-6 col-12">
                     {{ form_row(form.emailMembre, {'label': 'Email:', 'attr': {'class': 'form-control', 'placeholder': 'Email', 'required': 'required'}}) }}
                     {# Afficher les erreurs spécifiques au champ emailMembre #}
                 
                 </div>
                 
                 <div class="col-lg-6 col-12">
                     {{ form_row(form.fonction, {'label': 'Fonction:', 'attr': {'class': 'form-control', 'required': 'required'}}) }}
                     {# Afficher les erreurs spécifiques au champ fonction #}
             
                 </div>
                 <div class="col-lg-6 col-12">
                    {{ form_row(form.association, {'label': 'Association:', 'placeholder': 'Association', 'attr': {'class': 'form-control'}}) }}
                   
           
             </div>

             <br/>
             <button type="submit" class="btn btn-primary">Add member</button>
             {{ form_end(form) }} 
             {# Fin du formulaire #}
             
{% endblock %}
</div>
</div>


</body>
</html>
